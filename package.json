{
  "name": "ci4-bs5-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "sass src/scss/style.scss public/assets/css/style.css --source-map --load-path=node_modules",
    "build": "sass src/scss/style.scss public/assets/css/style.css --style=compressed --load-path=node_modules",
    "watch": "sass --watch src/scss/style.scss:public/assets/css/style.css --load-path=node_modules"
  },
  "dependencies": {
    "bootstrap": "^5.3.3"
  },
  "devDependencies": {
    "sass": "^1.75.0"
  }
}
